{
    "init.lua": {
        "alternate": "lua/plugins/init.lua"
    },
    "lua/plugins/init.lua": {
        "alternate": "init.lua",
        "type": "plugins"
    },
    "after/*": {
        "type": "after"
    },
    "autoload/*": {
        "type": "autoload"
    },
    "lua/plugins/*": {
        "type": "plugin"
    }
}
